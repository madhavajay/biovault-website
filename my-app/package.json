		"@tailwindcss/vite": "^4.1.18",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
